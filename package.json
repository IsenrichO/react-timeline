{
  "name": "react-timeline",
  "version": "1.0.0",
  "description": "Visualize timelines in a beautiful layout with React",
  "engineStrict": false,
  "main": "server.js",
  "scripts": {
    "build": "yarn prod:build",
    "clean": "rimraf *.log assets/common{.css*,.js*,.gz} assets/hmr{.css*,.js*,.gz} assets/polyfill{.css*,.js*,.gz} assets/vendor{.css*,.js*,.gz}",
    "db:dev": "mongod --httpinterface",
    "db:drop": "node db/drop.js",
    "db:rs": "brew services restart mongodb",
    "db:seed": "node db/seeds/seed.js",
    "db:start": "mongod --dbpath=/data/db",
    "db:start-alt": "brew services start mongodb",
    "db:stop": "brew services stop mongodb",
    "dev": "yarn mode:dev ./node_modules/webpack-dev-server/bin/webpack-dev-server -d",
    "dev:serve": "yarn mode:dev ./node_modules/webpack-dev-server/bin/webpack-dev-server -d --inline",
    "flow": "node_modules/.bin/flow check src/.",
    "flow:all": "node_modules/.bin/flow check --all",
    "flow:status": "node_modules/.bin/flow status",
    "flow:watch": "node_modules/onchange/cli.js 'src/**/*' -- node_modules/.bin/flow",
    "gen:stats": "webpack --json > stats.json",
    "lint": "eslint src",
    "mode:dev": "cross-env NODE_ENV=development BABEL_ENV=development",
    "mode:prod": "cross-env NODE_ENV=production BABEL_ENV=production",
    "mode:test": "cross-env NODE_ENV=test BABEL_ENV=test",
    "node:watch": "./node_modules/nodemon/bin/nodemon.js --config ./nodemon.json",
    "prod": "yarn mode:prod node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack",
    "prod:build": "yarn clean && yarn prod",
    "rf-mods": "rm -rf node_modules && yarn cache clean && yarn",
    "serve": "yarn mode:dev yarn dev",
    "serve:prod": "node --trace-warnings",
    "start": "yarn mode:dev yarn node:watch server",
    "start:stats": "yarn start STATS",
    "test": "yarn mode:test mocha test/**/*.js --compilers js:babel-register",
    "test:recursive": "yarn mode:test yarn node:watch --exec 'mocha --recursive -R min'",
    "test:watch": "yarn mode:test yarn node:watch --exec 'mocha test/**/*.js -R min'",
    "view:dbs": "mongo show dbs",
    "watch": "yarn mode:dev ./node_modules/webpack/bin/webpack --config ./webpack -w"
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=4.x",
    "yarn": ">=1.x"
  },
  "engines": {
    "node": ">=5.0.0 <=9.0.0"
  },
